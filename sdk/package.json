{
  "name": "pylinks-sdk",
  "version": "1.0.3",
  "description": "PyLinks SDK - Seamless PYUSD payment integration for merchants",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "keywords": [
    "pyusd",
    "payments",
    "web3",
    "crypto",
    "paypal",
    "stablecoin",
    "ethereum",
    "blockchain"
  ],
  "author": "PyLinks Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vmmuthu31/PyLinks.git",
    "directory": "sdk"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.6.2",
    "ethers": "5.7.2",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/qrcode": "^1.5.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
